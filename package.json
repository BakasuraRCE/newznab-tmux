        "@fortawesome/fontawesome-free": "^5.14.0",
        "animate.css": "^4.1.0",
        "autosize": "^4.0.2",
        "bootstrap": "^4.5.0",
        "bootstrap-datepicker": "^1.9.0",
        "bootstrap-hover-dropdown": "^2.2.1",
        "bootstrap-progressbar": "^0.9.0",
        "datatables": "^1.10.18",
